<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.healthink.user.healthink.Add_friend">

    <ImageView
        android:layout_width="73dp"
        android:layout_height="67dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="3dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/kontak_pictUser"
        android:layout_marginStart="16dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/kontak_displayName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/kontak_pictUser"
        app:layout_constraintLeft_toRightOf="@+id/kontak_pictUser"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/kontak_bio"
        android:layout_width="175dp"
        android:layout_height="48dp"
        app:layout_constraintTop_toBottomOf="@+id/kontak_displayName"
        app:layout_constraintLeft_toLeftOf="@+id/kontak_displayName" />

    <ImageView
        android:id="@+id/kontak_badge"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="3dp"
        app:layout_constraintLeft_toRightOf="@id/kontak_displayName" />


</android.support.constraint.ConstraintLayout>