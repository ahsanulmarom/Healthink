<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbarUser"
        android:layout_width="0dp"
        android:layout_height="69dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"


        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="0dp"
        android:layout_height="180dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/cover"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toBottomOf="@+id/toolbarUser"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:text="Profile"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/toolbarUser"
        app:layout_constraintLeft_toLeftOf="@+id/toolbarUser"
        app:layout_constraintRight_toRightOf="@+id/toolbarUser"
        app:layout_constraintTop_toTopOf="@+id/toolbarUser"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/user_pict"
        android:layout_width="102dp"
        android:layout_height="95dp"
        android:layout_marginTop="85dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbarUser"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView9"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/user_displayName"
        android:layout_width="162dp"
        android:layout_height="31dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="101dp"
        android:background="@color/colorPrimary"
        android:textAlignment="viewEnd"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/user_pict"
        app:layout_constraintTop_toBottomOf="@+id/toolbarUser"
        android:layout_marginStart="14dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="69dp"
        android:layout_height="45dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="4dp"
        android:background="@android:color/holo_orange_light"
        android:text="7000"
        android:textColor="@color/colorAccent"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/user_pict"
        app:layout_constraintTop_toBottomOf="@+id/user_displayName"
        android:layout_marginStart="14dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="32dp"
        android:fontFamily="monospace"
        android:text="Point"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintLeft_toLeftOf="@+id/textView13"
        app:layout_constraintRight_toRightOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="@+id/textView13"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="History"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_bio"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/user_bio"
        android:layout_width="0dp"
        android:layout_height="103dp"
        android:background="@drawable/roundbtntrans"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="156dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="154dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/user_pict" />


    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bio"
        android:textSize="24sp"
        android:textStyle="bold"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        android:layout_marginLeft="16dp" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar8"
        android:layout_width="385dp"
        android:layout_height="89dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:background="@color/common_google_signin_btn_text_dark_disabled" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="64dp"
        android:layout_height="64dp"
        app:srcCompat="@drawable/ic_artikel"
        android:layout_marginLeft="98dp"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar8"
        app:layout_constraintTop_toTopOf="@+id/toolbar8"
        android:layout_marginTop="0dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="64dp"
        android:layout_height="64dp"
        app:srcCompat="@drawable/ic_consultation"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        android:layout_marginBottom="0dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton2"
        android:layout_marginLeft="68dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Artikel"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2"
        android:layout_marginLeft="11dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton2" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Konsultasi"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        android:layout_marginLeft="-1dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton5" />


</android.support.constraint.ConstraintLayout>